<Window x:Class="LenkasLittleHelper.ReportDay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LenkasLittleHelper"
        mc:Ignorable="d"
        Title="ReportDay" Height="200" Width="200">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="9*"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
        </Grid.RowDefinitions>
        <DatePicker x:Name="ReportDay_Field" Grid.Row="0"></DatePicker>
        <Button x:Name="Report_Dt_Add" Click="Report_Dt_Add_Click" Content="Зберегти" Grid.Row="1" Margin="10"></Button>
    </Grid>
</Window>
