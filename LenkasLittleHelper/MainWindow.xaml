<Window x:Class="LenkasLittleHelper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Lenka's little helper" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button Content="Звіти" x:Name="Btn_Reports" Click="Btn_Reports_Click" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,5,0"/>
            <Button Content="Довідники" x:Name="Btn_Directories" Click="Btn_Directories_Click" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,5,0"/>
        </StackPanel>
    </Grid>
</Window>
